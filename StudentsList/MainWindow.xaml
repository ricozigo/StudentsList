<Window x:Class="StudentsList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentsList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnMainMenu" HorizontalAlignment="Left" Content="Main Menu" Click="btnMainMenu_Click"/>
        <Button Margin="250,0,0,0" x:Name="btnAddStudent" HorizontalAlignment="Left" Content="Add Student" Click="btnAddStudent_Click"/>
        <Frame Grid.Row="1" x:Name="mainFrame" NavigationUIVisibility="Hidden"></Frame>
        
    </Grid>
</Window>
